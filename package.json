  "scripts": {
    "tailwind:css": "yarn tailwind build src/css/tailwind.src.css -c  tailwind.js -o src/css/tailwind.css",
    "start:server": "cd server && ts-node-dev --respawn --transpileOnly ./server.ts && wait-on tcp:5000",
    "start:app": "yarn run tailwind:css && set HOST=0.0.0.0 && react-scripts-ts start",
    "start": "run-p --race start:server start:app",
    "build": "yarn run tailwind:css && react-scripts-ts build",
    "test": "react-scripts-ts test",
    "eject": "react-scripts-ts eject"
  },
  "browserslist": [
    ">0.2%",
